{
	"name": "kex-monorepo",
	"private": true,
	"engines": {
		"node": ">=20.0.0"
	},
	"scripts": {
		"start": "cd apps/api && npm start",
		"dev": "cd apps/api && npm run dev",
		"build": "echo 'no build needed for api'",
		"dev:api": "cd apps/api && npm run dev",
		"dev:web": "cd apps/web && npm run dev",
		"dev:all": "run-p dev:api dev:web"
	},
	"devDependencies": {
		"npm-run-all": "^4.1.5"
	}
} 